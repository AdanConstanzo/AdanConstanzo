
server {  
	listen 80 default_server; 

	server_name adanconstanzo.com www.adanconstanzo.com;

	location / {
		#proxy_http_version 1.1;
        #proxy_set_header Upgrade $http_upgrade;
        #proxy_set_header Connection 'upgrade';
        #proxy_set_header Host $host;
        #proxy_cache_bypass $http_upgrade;

		#root /home/danconable/Portfolio/front-end/build;
		root /var/www;
	} 
	location /api{
		proxy_pass http://localhost:8080;
	}
	location /hw2 {
		alias /var/www/WebBuild;
	}
	location /cam {
		# alias /var/www/cam;
	}
}
server {
	listen 80;
	
	root /var/www/BlogBuild;
	
	server_name blog.adanconstanzo.com; 

	location / {
		try_files $uri /index.html;
	}
	location /login {
		try_files $uri /index.html;
	}
	location  /api {
		proxy_pass http://localhost:8082;
		proxy_redirect     off;
		proxy_set_header   Host $host;
	}

}
server {
	listen 80;
	
	root /var/www/html;
	
	server_name login.adanconstanzo.com; 	
	location  / {
		proxy_pass http://localhost:8083;
		proxy_redirect     off;
		proxy_set_header   Host $host;
	}
	
}


